/* component/header/header.wxss */

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: linear-gradient(0deg,#FFE300,#FFAE01);
  color: #333;
  z-index: 10000;
}

.status-bar {
  
}

.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title-bar .back-btn, .title-bar .tablet {
  width: 116px; /* 保持一致使标题居中 */
  height: 100%;
}

.title_icon{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100%;
}
.backBtn{
  height: 100%;
  width: 78rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.backBtn>.iconfont{
  color: #fff;
  font-size: 38rpx;
}
.homeBtn{
  height: 100%;
  width: 78rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.homeBtn>.iconfont{
  color: #fff;
  font-size: 43rpx;
}
.icon_line{
  height: 18px;  border-left: 1px solid #fff;
}

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1536660825828'); /* IE9*/
  src: url('iconfont.eot?t=1536660825828#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1536660825828') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1536660825828#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jia:before { content: "\e600"; }

.icon-you:before { content: "\e65f"; }

.icon-guanbi:before { content: "\e6b5"; }

.icon-shouye:before { content: "\e6de"; }

.icon-fanhui:before { content: "\e837"; }
